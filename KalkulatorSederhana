    private void btnTitikActionPerformed(java.awt.event.ActionEvent evt) {                                         
        angka += ",";
        tampilHasil.setText(angka);
    }                                        

    private void btnResetActionPerformed(java.awt.event.ActionEvent evt) {                                         
        tampilHasil.setText(angka);
        bil1 = 0.0;
        bil2 = 0.0;
        jumlah = 0.0;
        angka = "";
    }                                        

    private void btnTambahActionPerformed(java.awt.event.ActionEvent evt) {                                          
        bil1 = Double.parseDouble(angka);
        tampilHasil.setText("+");
        angka = "";
        pilih = 1;
    }                                         

    private void btnKurangActionPerformed(java.awt.event.ActionEvent evt) {                                          
        bil1 = Double.parseDouble(angka);
        tampilHasil.setText("-");
        angka = "";
        pilih = 2;
    }                                         

    private void btnKaliActionPerformed(java.awt.event.ActionEvent evt) {                                        
        bil1 = Double.parseDouble(angka);
        tampilHasil.setText("x");
        angka = "";
        pilih = 3;
    }                                       

    private void btnBagiActionPerformed(java.awt.event.ActionEvent evt) {                                        
        bil1 = Double.parseDouble(angka);
        tampilHasil.setText("/");
        angka = "";
        pilih = 4;
    }                                       

    private void btnModActionPerformed(java.awt.event.ActionEvent evt) {                                       
        bil1 = Double.parseDouble(angka);
        tampilHasil.setText("%");
        angka = "";
        pilih = 5;
    }                                      

    private void btnSamadenganActionPerformed(java.awt.event.ActionEvent evt) {                                              
        switch (pilih){
            case 1:
            bil2 = Double.parseDouble(angka);
            jumlah = bil1 + bil2;
            angka = String.valueOf((int) jumlah); // Mengubah hasil menjadi integer
            tampilHasil.setText(angka);
            break;
        case 2:
            bil2 = Double.parseDouble(angka);
            jumlah = bil1 - bil2;
            angka = String.valueOf((int) jumlah); // Mengubah hasil menjadi integer
            tampilHasil.setText(angka);
            break;
        case 3:
            bil2 = Double.parseDouble(angka);
            jumlah = bil1 * bil2;
            angka = String.valueOf((int) jumlah); // Mengubah hasil menjadi integer
            tampilHasil.setText(angka);
            break;
        case 4:
            bil2 = Double.parseDouble(angka);
            jumlah = bil1 / bil2;
            angka = String.valueOf((int) jumlah); // Mengubah hasil menjadi integer
            tampilHasil.setText(angka);
            break;
        case 5:
            bil2 = Double.parseDouble(angka);
            jumlah = bil1 % bil2;
            angka = String.valueOf((int) jumlah); // Mengubah hasil menjadi integer
            tampilHasil.setText(angka);
            break;
        default:
            break;
        }
    }                                             

